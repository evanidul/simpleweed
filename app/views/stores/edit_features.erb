                  <tr>
                    <td>
                      <%= f.label :twentyoneplus, "21 +" %><br>
                    </td>
                    <td>
                      <%= f.check_box :twentyoneplus, {}, "true","false" %>
                    </td>
                  </tr>
                  <tr>
                    <td>
                      <%= f.label :hasphotos, "photos" %><br>
                    </td>
                    <td>
                      <%= f.check_box :hasphotos, {}, "true","false" %>
                    </td>
                  </tr>                  
                  <tr>
                    <td>
                      <%= f.label :onsitetesting, "on site testing" %><br>
                    </td>
                    <td>
                      <%= f.check_box :onsitetesting, {}, "true","false" %>
                    </td>
                  </tr>                  
                </table>
                <br />
                <br />
                
                    
              <p class = "form-group pull-right">               
                <button type="button" class="btn btn-default" data-dismiss="modal">close</button>
                <%= f.submit :class => 'btn btn-primary lowercase' , :id => 'save_store_features_button'%>
              </p>
            <% end %>
            <div class="simple-weed-modal-footer">
            </div>
          </div><!-- modal-footer -->
      </div> <!-- modal-content -->
